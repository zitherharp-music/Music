<Window x:Class="Wpf.Views.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:basic="clr-namespace:Wpf.Views.Basic"
        xmlns:vertical="clr-namespace:Wpf.Views.Vertical"
        xmlns:horizontal="clr-namespace:Wpf.Views.Horizontal"
        Title="Zither Harp Music: Television" WindowState="Maximized">
    <Grid>
        <WebBrowser x:Name="mPlayerView" Visibility="Hidden"/>
        <basic:BasicScreen x:Name="mBasicScreen" Visibility="Hidden"/>
        <vertical:VerticalScreen x:Name="mVerticalScreen" Visibility="Hidden"/>
        <horizontal:HorizontalScreen x:Name="mHorizontalScreen" Visibility="Hidden"/>
    </Grid>
</Window>
